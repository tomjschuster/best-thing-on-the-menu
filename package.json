{
  "name": "best-thing-on-the-menu",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run db-up-prod && node server/index.js",
    "postinstall": "webpack --display-error-details",
    "start:dev": "NODE_ENV=development nodemon server/index.js",
    "build": "webpack",
    "build-watch": "NODE_ENV=development webpack -w",
    "db-create": "db-migrate db:create btotm --config './db/database.json' --env localhost --migrations-dir './db/migrations'",
    "db-drop": "db-migrate db:drop btotm --config './db/database.json' --env localhost --migrations-dir './db/migrations'",
    "db-up-prod": "db-migrate up --config ./db/database.json --env 'prod'",
    "db-up": "db-migrate up  --config './db/database.json' --migrations-dir './db/migrations'",
    "db-down": "db-migrate reset --config './db/database.json' --migrations-dir './db/migrations'",
    "db-seed": "db-migrate up  --config './db/database.json' --migrations-dir './db/seeds' --table 'seeds'",
    "db-reset": "npm run db-drop && npm run db-create && npm run db-up && npm run db-seed",
    "db-migration-check": "npm run db-drop && npm run db-create && npm run db-up && npm run db-seed && npm run db-down && npm run db-up && npm run db-seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomjschuster/best-thing-on-the-menu.git"
  },
